{
  "node_name": "${NODE_NAME}",
  "server": true,
  "ui_config": {
    "enabled": true
  },
  "data_dir": "/consul/data",
  "addresses": {
    "http": "0.0.0.0"
  },
  "retry_join": ${JOIN_HOSTS},
  "acl": {
    "enabled": true,
    "default_policy": "deny",
    "enable_token_persistence": true,
    "tokens": {
      "master": "e95b599e-166e-7d80-08ad-aee76e7ddf19",
      "agent": "e95b599e-166e-7d80-08ad-aee76e7ddf19"
    }
  },
  "connect": { "enabled": true },
  "auto_config": {
    "authorization": {
      "enabled": true,
      "static": {
        "jwt_validation_pub_keys": [
          "-----BEGIN CERTIFICATE-----\nMFkwEwYHKoZIzj0CAQYIKoZIzj0DAQcDQgAEsroNIWasf/FbIbwFwoYx5RxKL+cKsKJ5UPej+SQ7taJryoK1Cu1I5yvRPrFAW0anyDzVL8ZOASBCDuEp37h95g==\n-----END CERTIFICATE-----\n"
        ],
        "bound_issuer": "auto-config-cluster",
        "bound_audiences": ["audience"],
        "claim_mappings": {
          "sub": "node"
