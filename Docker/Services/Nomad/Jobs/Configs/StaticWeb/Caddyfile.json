{
  "apps": {
    "http": {
      "http_port": 8080,
      "https_port": 8443,
      "servers": {
        "consul": {
          "listen": [
            "0.0.0.0:{{ env "NOMAD_PORT_http" }}"
          ],
          "automatic_https": {
            "disable": true
          },
          "routes": [
            {
              "handle": [
                {
                  "handler": "static_response",
                  "status_code": "200",
                  "body": "\nStaticWeb\n{{ env "NOMAD_JOB_ID" }}",
                  "close": true
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  }
}