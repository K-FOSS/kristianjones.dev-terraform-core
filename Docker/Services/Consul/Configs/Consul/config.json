{
  "ui_config": {
    "enabled": true
  },
  "addresses": {
    "http": "0.0.0.0"
  },
  "ports": {
    "dns": 9600,
    "http": 9500,
    "https": 9501,
    "grpc": 9502,
    "server": 9300
  },
  "acl": {
    "enabled": true,
    "default_policy": "deny",
    "enable_token_persistence": true,
    "tokens": {
      "master": "${TOKENS.MASTER_TOKEN}",
      "default": "${TOKENS.DEFAULT_TOKEN}",
      "agent": "${TOKENS.AGENT_TOKEN}",
      "agent_master": "${TOKENS.AGENT_MASTER_TOKEN}",
      "replication": "${TOKENS.REPLICATION_TOKEN}"
    }
  },
  "connect": { "enabled": true },
  "autopilot": {
    "cleanup_dead_servers": true
  },
  "encrypt": "${SECRET_KEY}",
  "verify_incoming": false,
  "verify_outgoing": false,
  "verify_server_hostname": false,
  "rpc": {
    "enable_streaming": true
  },
  "use_streaming_backend": true,

  "telemetry": {
    "disable_compat_1.9": true
  }
}
